{
    "name": "silverstripe/sspak",
    "description": "CLI tool for saving & loading data in SilverStripe installations",
    "type": "library",
    "require": {
        "symfony/console": "^3",
        "league/flysystem": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "symfony/process": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sam Minnee",
            "email": "sam@silverstripe.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SilverStripe\\SSPak\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilverStripe\\Test\\SSPak\\": "tests/"
        }
    },
    "bin": [
        "bin/sspak"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
